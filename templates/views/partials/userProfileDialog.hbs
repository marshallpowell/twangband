<!-- start user profile partial -->
<script id="userProfilePartial" type="text/x-handlebars-template">
    <div class="person">
        <div class="row">
            <div class="col-md-4">
                <img src="/uploads/users/profile/\{{userDto.profilePic}}" width="100" height="100"/>
            </div>
            <div class="col-md-8">
                <div class="row"><h2>\{{userDto.firstName}} \{{userDto.lastName}}</h2></div>
                <div class="row">
                    <div class="userProfileTags">
                        \{{#each userDto.tags}}
                        <button type="button" class="btn btn-primary btn-xs" disabled>\{{ this }}</button>
                        \{{/each}}
                    </div>
                </div>
            </div>
        </div>


    </div>
</script>

<div id="userProfileDialog" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                {{> siteName }}
                <button type="button" class="close" id="modalXCloseBtn" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body ">
                <div id="userProfileBody"><p id="userProfileFoo"></p></div>
            </div>

        </div>

    </div>
</div>
<!-- end user profile partial -->