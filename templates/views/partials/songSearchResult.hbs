{{#if clientSide}}

<!-- start song search result -->
<script id="songSearchResultPartial" type="text/x-handlebars-template" xmlns="http://www.w3.org/1999/html">
<div class="row searchResultRow">
    <div class="col-md-9" >

        <div class="row searchRowTitle">
            <div class="col-md-12" >
                <div style='float:left; padding:10px;'>
                    <button class="btn btn-primary btn-circle" onclick="tb.dialogs.openSongInfoDialog(\{{json searchResult.song}});" >
                        <i class="glyphicon glyphicon-play"></i>
                    </button>
                </div>
                <div style='float:left; padding-top:13px; line-height: 90%;'>
                    <a  href="/songMixer?song=\{{searchResult.song.id}}">\{{searchResult.song.name}}</a><br />
                    <span class="searchDuration">\{{searchResult.song.duration}} seconds, created: \{{formatDate searchResult.song.dateCreated}}</span>
                </div>
            </div>
        </div>
        <div class="row" style="overflow: hidden">
            \{{#if searchResult.song.fileName}}
                <div class="col-md-12">
                    <div id="wav_\{{searchResult.song.id}}"><p id="wav_loading_\{{searchResult.song.id}}"><span class="fa fa-spinner fa-pulse"></span> Loading...</p></div>
                    <div id="wavImg_\{{searchResult.song.id}}" class="searchRowWav"></div>

                </div>
            \{{/if}}
            \{{#unless searchResult.song.fileName}}
                <div class="col-md-12 searchNoInfo">
                    No Tracks Recorded Yet
                </div>
            \{{/unless}}
        </div>
    </div>
    <div class="col-md-3 ">

        <div class="row searchResultExtraInfo">
            <div class="searchResultSideLabel"> musicians: </div>

            \{{#each searchResult.musicians}}
                <div style='float:left;'>
                   \{{> musicianInfoIcon userDto=this}}
                </div>
            \{{/each}}
        </div>

        \{{#if searchResult.song.allTags}}
            <div class="row searchResultExtraInfo">
                <div class="searchResultSideLabel"> tags: </div>
                <div style='float:left;'>
                    \{{#each searchResult.song.allTags}}
                        \{{> tagInfo tag=this}}
                    \{{/each}}
                </div>
            </div>
        \{{/if}}


    </div>
</div>
</script>
<!-- end song search result -->
{{/if}}

